sudo cat /etc/zabbix/zabbix_server.conf
DBHost=localhost
DBName=zabbix
DBUser=zabbix
DBPassword=zabbix
PidFile=/var/run/zabbix/zabbix_server.pid
LogFile=/var/log/zabbix/zabbix_server.log
StartReportWriters=1
WebServiceURL=http://127.0.0.1:10053/report
ListenIP=127.0.0.1
ListenPort=10051

sudo cat /etc/zabbix/zabbix_web_service.conf
# Zabbix Web Service configuration
AllowedIP=127.0.0.1
ListenPort=10053
LogFile=/var/log/zabbix_web_service/zabbix_web_service.log
TLSAccept=unencrypted

For Test
# Zabbix Ser configuration
DBHost=localhost
DBName=zabbix
DBUser=zabbix
DBPassword=zabbix
PidFile=/var/run/zabbix/zabbix_server.pid
LogFile=/var/log/zabbix/zabbix_server.log
StartReportWriters=1
WebServiceURL=http://localhost:10053/report
ListenIP=127.0.0.1
ListenPort=10051
AllowedIP=127.0.0.1,::1

# Zabbix Web Service configuration
AllowedIP=127.0.0.1
ListenPort=10053
LogFile=/var/log/zabbix_web_service/zabbix_web_service.log
TLSAccept=unencrypted 
